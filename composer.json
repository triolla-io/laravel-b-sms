{
    "name": "yna/laravel-b-sms",
    "description": "B SMS Notifications channel for Laravel 5.3.",
    "keywords": ["laravel", "notifications", "b-sms"],
    "homepage": "https://github.com/ynacorp/b-sms",
    "license": "MIT",
    "authors": [
        {
            "name": "Shemi",
            "role": "Developer",
            "homepage": "https://github.com/shemi"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/queue": "^5.1.0",
        "illuminate/notifications": "^5.1.0",
        "illuminate/support": "^5.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "Yna\\BSms\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yna\\BSms\\BSmsServiceProvider"
            ]
        }
    }
}
